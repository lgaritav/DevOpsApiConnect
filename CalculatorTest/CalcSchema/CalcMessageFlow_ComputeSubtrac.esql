BROKER SCHEMA CalcSchema


CREATE COMPUTE MODULE CalcMessageFlow_ComputeSubtrac
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		DECLARE REFIN  REFERENCE TO InputRoot.XMLNSC;
		CREATE FIELD OutputRoot.XMLNSC.ns:SubtractResponse;
		DECLARE REFOUT REFERENCE TO OutputRoot.XMLNSC.ns:SubtractResponse;
		SET REFOUT.SubtractResult = (CAST(REFIN.ns:Subtract.intA AS INTEGER) - CAST(REFIN.ns:Subtract.intB AS INTEGER));

	RETURN TRUE;
	END;
END MODULE;
